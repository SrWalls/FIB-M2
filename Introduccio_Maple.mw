<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Text" size="16" bold="true" family="Arial" layout="Heading 2"><Font bold="true" family="Arial" encoding="UTF-8" size="16">Seq\303\274\303\250ncies, conjunts i llistes</Font></Text-field></Title>
<Group labelreference="L33" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name222"><Font encoding="UTF-8">Per construir una seq\303\274\303\250ncia de valors</Font></Text-field>
</Input>
</Group>
<Group labelreference="L34" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">seq(1/k, k = 1 .. 10);</Text-field>
</Input>
</Group>
<Group labelreference="L35" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name223">Delimita una llista</Text-field>
</Input>
</Group>
<Group labelreference="L36" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">l := [seq(1/(k^2+1), k = -10 .. 10)];</Text-field>
</Input>
</Group>
<Group labelreference="L37" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name224">Delimita un conjunt</Text-field>
</Input>
</Group>
<Group labelreference="L38" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">c := {seq(1/(k^2+1), k = -10 .. 10)};</Text-field>
</Input>
</Group>
<Group labelreference="L39" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name225"><Font encoding="UTF-8">Nombre d'elements de l'espressi\303\263</Font></Text-field>
</Input>
</Group>
<Group labelreference="L40" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">nops(l); nops(c);</Text-field>
</Input>
</Group>
<Group labelreference="L41" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name226"><Font encoding="UTF-8">Element n-\303\250ssim de l'expressi\303\263</Font></Text-field>
</Input>
</Group>
<Group labelreference="L42" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">op(3, l); op(3, c);</Text-field>
</Input>
</Group>
<Group labelreference="L43" drawlabel="true">
<Input>
<Text-field style="Text" layout="No Name227">Podem aplicar funcions i operacions a tots els elements d'una llista o d'un conjunt</Text-field>
</Input>
</Group>
<Group labelreference="L44" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">map(sqrt, c); map(evalf, %);</Text-field>
</Input>
